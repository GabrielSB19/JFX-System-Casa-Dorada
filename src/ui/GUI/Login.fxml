<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="bpMain" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: #080404;">
         <children>
            <Pane layoutY="1.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: #fee99b;">
               <children>
                  <ImageView fx:id="ivLoginP" fitHeight="474.0" fitWidth="366.0" pickOnBounds="true" smooth="false" />
                  <Label layoutX="112.0" layoutY="54.0" text="Inicia Sesión">
                     <font>
                        <Font name="SansSerif Regular" size="25.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtUser" alignment="CENTER" layoutX="59.0" layoutY="132.0" promptText="Usuario" style="-fx-background-color: #000000;">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </TextField>
                  <PasswordField fx:id="txtPassword" alignment="CENTER" layoutX="59.0" layoutY="204.0" promptText="Contraseña" style="-fx-background-color: #000000;">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </PasswordField>
                  <Button layoutX="109.0" layoutY="287.0" mnemonicParsing="false" onAction="#onLogIn" prefHeight="56.0" prefWidth="148.0" text="Inicia Sesion">
                     <font>
                        <Font name="SansSerif Regular" size="20.0" />
                     </font>
                  </Button>
                  <Button layoutX="93.0" layoutY="365.0" mnemonicParsing="false" onAction="#onSingGo" prefHeight="20.0" prefWidth="180.0" text="¿Aún no tienes cuenta?" underline="true">
                     <font>
                        <Font name="SansSerif Regular" size="15.0" />
                     </font>
                  </Button>
                  <Button layoutX="131.0" layoutY="400.0" mnemonicParsing="false" onAction="#onIhaveAccount" text="Ya tengo cuenta" underline="true" />
               </children>
            </Pane>
         </children>
      </Pane>
   </center>
</BorderPane>
